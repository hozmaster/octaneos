###############################################################
# bochsrcfile for Orange Linux Image
# Configuration Updated: 11/18/2007
###############################################################

# how much memory the emulated machine will have
megs: 32

# filename of ROM images
# Edit to your bochs HOME directory
romimage: file=../bochs_bios/bochs-2.3.5/bios/BIOS-bochs-latest
vgaromimage: file=../bochs_bios/bochs-2.3.5/bios/VGABIOS-lgpl-latest

# what disk images will be used 
floppya: 1_44=orange.img, status=inserted
floppyb: 1_44=floppyb.img, status=inserted

# hard disk - not enabled
# ata0-master: type=disk, path="30M.sample", cylinders=615, heads=6, spt=17

# choose the boot disk.
boot: a

# Send log messages?
log: bochsout.txt

# disable the mouse, since our image is text only
mouse: enabled=0
